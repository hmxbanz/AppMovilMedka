<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:bottomLeftRadius="@dimen/watchRadiu" android:bottomRightRadius="@dimen/watchRadiu"/>
            <gradient android:startColor="@color/progressbag" android:endColor="@color/progressbag" android:angle="0"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:bottomLeftRadius="@dimen/watchRadiu" android:bottomRightRadius="@dimen/watchRadiu"/>
                <gradient android:startColor="@color/main" android:endColor="@color/main" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
