<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/email_login_button_normal"/>
            <stroke android:width="@dimen/line_height" android:color="#b3b3b4"/>
            <corners android:radius="90dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/email_login_button_press"/>
            <stroke android:width="@dimen/line_height" android:color="#b3b3b4"/>
            <corners android:radius="90dp"/>
        </shape>
    </item>
</selector>
