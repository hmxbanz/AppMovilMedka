                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/map_average_speed_colon" android:layout_marginStart="25dp"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:id="@+id/txtAvgHeart" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="--"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/map_maximum_rate_colon" android:layout_marginStart="25dp"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:id="@+id/txtHighHeart" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="--"/>
