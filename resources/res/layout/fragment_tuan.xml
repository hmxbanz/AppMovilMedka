            </LinearLayout>
            <com.yc.pedometer.sports.widget.PolyLineFloatDetailChartBak android:id="@+id/heartPolyChart" android:paddingTop="10dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="200dp" android:layout_marginTop="20dp" android:layout_below="@+id/toolbar" android:paddingEnd="5dp" android:layout_marginStart="@dimen/marginLeft" android:layout_marginEnd="@dimen/marginRight" app:slideMax="160" app:slideMin="50" app:slideShowNum="3" app:strokeColor="#ff944052" app:strokeSize="1.5dp"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/llaltitude" android:background="#222433" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_marginBottom="10dp">
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:paddingEnd="15dp" android:layout_marginStart="@dimen/marginLeft">
                <ImageView android:background="@drawable/haiba_detail" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/map_altitude" android:layout_marginStart="5dp"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/map_climb_colon" android:layout_marginStart="25dp"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:id="@+id/txtPaSheng" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/map_decline_colon" android:layout_marginStart="25dp"/>
                <TextView android:textSize="@dimen/text_size_2" android:textColor="@color/litterGray" android:id="@+id/txtXiaJiang" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0"/>
