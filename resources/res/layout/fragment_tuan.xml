            </LinearLayout>
            <com.yc.pedometer.sports.widget.HaiBaFloatDetailChartBak android:id="@+id/haibaPolyChart" android:paddingTop="10dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="200dp" android:layout_marginTop="20dp" android:layout_below="@+id/heartPolyChart" android:paddingEnd="5dp" android:layout_marginStart="@dimen/marginLeft" android:layout_marginEnd="@dimen/marginRight" app:slideShowNum="3" app:strokeColor="#ff508342" app:strokeSize="1.5dp"/>
        </LinearLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
